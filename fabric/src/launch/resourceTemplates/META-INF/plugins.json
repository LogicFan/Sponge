{
    "plugins": [
        {
            "loader": "java_plain",
            "id": "minecraft",
            "name": "Minecraft",
            "version": "${minecraftVersion}",
            "main-class": "org.spongepowered.common.launcher.Launcher",
            "links": {
                "homepage": "https://www.minecraft.net"
            },
            "contributors": [
                {
                    "name": "Mojang AB",
                    "description": "Lead Developer"
                }
            ],
            "dependencies": [],
            "extra": {}
        },
        {
            "loader": "java_plain",
            "id": "spongeapi",
            "name": "SpongeAPI",
            "version": "${apiVersion}",
            "main-class": "org.spongepowered.api.Sponge",
            "description": "The Minecraft API specification",
            "links": {
                "homepage": "https://www.spongepowered.org",
                "source": "https://www.spongepowered.org/source",
                "issues": "https://www.spongepowered.org/issues"
            },
            "contributors": [
                {
                    "name": "SpongePowered",
                    "description": "Lead Developer"
                }
            ],
            "dependencies": [],
            "extra": {}
        },
        {
            "loader": "java_plain",
            "id": "sponge",
            "name": "Sponge",
            "version": "${minecraftVersion}-${apiVersion}",
            "main-class": "org.spongepowered.common.SpongeCommon",
            "description": "The common Sponge implementation",
            "links": {
                "homepage": "https://www.spongepowered.org",
                "source": "https://www.spongepowered.org/source",
                "issues": "https://www.spongepowered.org/issues"
            },
            "contributors": [
                {
                    "name": "SpongePowered",
                    "description": "Lead Developer"
                }
            ],
            "dependencies": [
                {
                    "id": "minecraft",
                    "version": "${minecraftVersion}"
                },
                {
                    "id": "spongeapi",
                    "version": "${apiVersion}"
                }
            ],
            "extra": {}
        },
        {
            "loader": "java_plain",
            "id": "spongefabric",
            "name": "SpongeFabric",
            "version": "${version}",
            "main-class": "org.spongepowered.fabric.SpongeFabric",
            "description": "Sponge Implementation for FabricMC",
            "links": {
                "homepage": "https://www.spongepowered.org",
                "source": "https://www.spongepowered.org/source",
                "issues": "https://www.spongepowered.org/issues"
            },
            "contributors": [
                {
                    "name": "SpongePowered",
                    "description": "Lead Developer"
                }
            ],
            "dependencies": [
                {
                    "id": "sponge",
                    "version": "${minecraftVersion}-${apiVersion}"
                }
            ],
            "extra": {}
        }
    ]
}